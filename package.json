{
  "name": "TrailTales",
  "version": "1.0.0",
  "description": "An app for logging sightings (fungi, fauna, flora) during walks",
  "main": ".prettier.config.js",
  "scripts": {
    "build": "eleventy --quiet && tailwindcss -i ./src/assets/styles/styles.css -o ./www/assets/styles/styles.css --minify",
    "serve": "eleventy --serve --quiet",
    "debug": "node --inspect node_modules/.bin/eleventy --serve --incremental",
    "css": "tailwindcss -i ./src/assets/styles/styles.css -o ./www/assets/styles/styles.css --watch"
  },
  "keywords": [],
  "author": "Jan De Wilde",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-bundle": "^3.0.0",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@tailwindcss/typography": "^0.5.15",
    "js-yaml": "^4.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-jinja-template": "^2.0.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13"
  }
}
